<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" stylesheets="@font_size.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dudzinski.kacper.farec.controllers.NewEdgeLabelWindowController">
   <children>
      <Label alignment="TOP_CENTER" maxWidth="1.7976931348623157E308" text="Enter the new label below:" textAlignment="CENTER" />
      <TextField fx:id="textField" alignment="CENTER" />
      <HBox alignment="CENTER" spacing="10.0">
         <children>
          <Button alignment="CENTER" focusTraversable="false" minWidth="80.0" mnemonicParsing="false" onAction="#emptyString" text="Empty String" textAlignment="CENTER" />
            <Button fx:id="submitButton" alignment="CENTER" disable="true" minWidth="80.0" mnemonicParsing="false" onAction="#submit" text="Submit" textAlignment="CENTER" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
