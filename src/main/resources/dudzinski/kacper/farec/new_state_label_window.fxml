<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0"
      stylesheets="@font_size.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="dudzinski.kacper.farec.controllers.NewStateLabelWindowController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <Label alignment="TOP_CENTER" maxWidth="1.7976931348623157E308" text="Enter the new label below:"
          textAlignment="CENTER"/>
   <TextField fx:id="textField" alignment="CENTER"/>
   <Button alignment="CENTER" mnemonicParsing="false" onAction="#submit" text="Submit" textAlignment="CENTER"/>
</VBox>
